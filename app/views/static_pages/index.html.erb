<h1>Flickr API</h1>
<%= form_with method:"get" do |f| %>
<%= f.label :id, "ID:" %>
<%= f.text_field :id %>
<%= f.submit "Search" %>
<% end %>

<% @images.each do |image| %>
    <%= image_tag "http://farm#{image['farm']}.staticflickr.com/#{image['server']}/#{image['id']}_#{image['secret']}.jpg" %>
<% end %>
